
<!doctype html>
<html lang="en">


<head>

    <meta charset="utf-8" />
    <title>VSaver Admin Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />*@
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/Template/assets/images/favicon.ico">

    <!-- Bootstrap Css -->
    <link href="~/Content/Template/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/Content/Template/assets/css/line.css" rel="stylesheet" />

    <link href="~/Content/Template/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
