        <!-- Header section -->
<header class="header_1" id="header">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-3 col-md-3">
                <div class="logo">
                    <a href=""><img src="~/Content/UserUI/images/Vision-Savings-Transparent.png" alt=""></a>
                </div>
            </div>
            <div class="col-lg-7 col-md-7">
                <nav class="mainmenu MenuInRight text-right">
                    <a href="javascript:void(0);" class="mobilemenu d-md-none d-lg-none d-xl-none">
                        <span></span>
                        <span></span>
                        <span></span>
                    </a>
                    <ul>
                        <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                        <li class="menu-item-has-children">
                            <a href="#">Services</a>
                            <ul class="sub-menu">
                                <li><a href="">Service 01</a></li>
                                <li><a href="">Service 02</a></li>
                            </ul>
                        </li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li>@Html.ActionLink("Dashboard", "Index", "Agent")</li>
                        }
                        <li><a href="">Contact</a></li>

                    </ul>
                </nav>
            </div>
            <div class="col-lg-2 col-md-2 hidden-xs">
                <div class="navigator_btn btn_bg text-right">
                    <ul>
                        @if (User.Identity.IsAuthenticated)
                        {
                            using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                            {
                                @Html.AntiForgeryToken()
                                    <li><a class="common_btn" href="javascript:document.getElementById('logoutForm').submit()">Log Out</a></li>
                            }
                        }
                        else
                        {
                            <li><a class="common_btn" href="@Url.Action("Login", "Account")">Login</a></li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
</header>
<!-- Header section -->